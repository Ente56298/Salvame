{
  "name": "multi-agent-chat",
  "displayName": "Multi-Agent Chat",
  "description": "Chat colaborativo con Amazon Q, Copilot y Gemini",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multiAgent.openChat",
        "title": "Abrir Multi-Agent Chat"
      }
    ],
    "keybindings": [
      {
        "command": "multiAgent.openChat",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}